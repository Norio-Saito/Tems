<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.devMode" value="true" />

	<package name="com.stny.tems.action"  extends="struts-default">

<!-- トレーニングをDBに登録するアクション -->
<action name="InsertTraningAction" class="com.stny.tems.action.InsertTraningAction" method="execute">
	<result name="success">traning.jsp</result>
	<result name="error">upload.jsp</result>
</action>

<!-- トレーニングを表示するアクション -->
<action name="SelectTraningAction" class="com.stny.tems.action.InsertTraningAction" method="execute">
	<result name="success">traning.jsp</result>
	<result name="error">upload.jsp</result>
</action>
	</package>
</struts>
